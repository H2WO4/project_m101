{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2015",
    "lib": ["ES2020", "DOM"],
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": true
  },
  "include": [
    "dashboard.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
